{
  "name": "todolist",
  "version": "1.0.0",
  "description": "This ToDo app allows users to add/delete/update and view tasks ",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "mongoose": "^5.5.11",
    "react": "^16.2.0",
    "react-datepicker": "^2.6.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts-ts": "2.8.0",
    "redux": "^3.7.2",
    "redux-saga": "^1.0.2",
    "reselect": "^3.0.1",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "eject": "react-scripts-ts eject",
    "start-dev": "concurrently \"npm run server\" \"npm run dev\" ",
    "server": "node -r esm src/server/server",
    "dev": "react-scripts-ts start",
    "test-db": "ts-node -r esm src/server/connect-db.ts",
    "test-server": "ts-node -r esm src/server/server.ts",
    "test-server-script": "npx ts-node-dev -r esm src/server/server.spec.ts",
    "start-docs": "typedoc --out docs src/"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^21.1.9",
    "@types/mongodb": "^3.1.27",
    "@types/mongoose": "^5.5.3",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-datepicker": "^2.3.0",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/redux-logger": "^3.0.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "mocha": "^6.1.4",
    "mongodb": "^3.2.6",
    "raf": "^3.4.1",
    "redux-logger": "^3.0.6",
    "sinon": "^7.3.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typedoc": "^0.14.2",
    "typings": "^2.1.1"
  },
  "author": "Monu Chacko",
  "license": "ISC"
}
